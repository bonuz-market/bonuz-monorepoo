{
	"private": true,
	"scripts": {
		"dev:bonuz-id": "turbo run dev --filter=bonuz-id",
		"dev:admin-bonuz-xyz": "turbo run dev --filter=admin-bonuz-xyz",
		"install:in": "pnpm add --filter ",
		"build": "turbo run build",
		"clean": "turbo run clean",
		"dev": "turbo run dev",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"lint": "turbo run lint",
		"test": "turbo run test",
		"typecheck": "turbo run typecheck"
	},
	"devDependencies": {
		"prettier": "^3.2.5",
		"turbo": "^1.12.4"
	},
	"engines": {
		"node": ">=18"
	},
	"pnpm": {
		"overrides": {
			"valtio": "v1.10.5",
			"abitype": "v1.0.0"
		}
	}
}
