    "all:deploy:local": "npx hardhat deploy --network hardhat",
    "all:deploy:goerli": "npx hardhat deploy --network goerli",
    "all:deploy:polygon_mumbai": "npx hardhat deploy --network polygon_mumbai",
    "all:test:local": "hardhat test",
    "all:test:local:verbose": "hardhat test --no-compile --verbose",
    "all:test2:local": "hardhat test ./test/unit/*.ts --network hardhat",
    "==================== BonuzSocialId Contract ====================": "",
    "NOTE_1": "make sure to run local node by running `pnpm node` to deploy it locally",
    "bonuzSocialId:deploy:local": "npx hardhat deploy --network localhost --tags BonuzSocialId",
    "bonuzSocialId:deploy:hardhat": "npx hardhat deploy --network hardhat --tags BonuzSocialId",
    "bonuzSocialId:test:local": "hardhat test ./test/unit/01-bonuz-social-id.ts",
