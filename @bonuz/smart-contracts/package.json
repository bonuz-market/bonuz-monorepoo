    "bonuzTokens:deploy:goerli": "npx hardhat deploy --network goerli --tags BonuzTokens",
    "bonuzTokens:deploy:polygon_mumbai": "npx hardhat deploy --network polygon_mumbai --tags BonuzTokens",
