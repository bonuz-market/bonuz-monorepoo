    "bonuzTokens:deploy:base_mainnet": "npx hardhat deploy --network base_mainnet --tags BonuzTokens",
    "bonuzTokens:deploy:base_sepolia_testnet": "npx hardhat deploy --network base_sepolia_testnet --tags BonuzTokens",
    "==================== BonuzFuel Contract ====================": "",
    "bonuzFuel:deploy:polygon_mumbai": "npx hardhat deploy --network polygon_mumbai --tags Bfuel",
    "==================== bonuz Contract ====================": "",
    "bonuz:test:local": "hardhat test ./test/unit/03-bonuz.test.ts",
    "==================== TOOLS ====================": "",
    "lint": "solhint contracts/*.sol",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix",
    "format": "prettier --write .",
    "coverage": "hardhat coverage",
    "coverage2": "hardhat coverage --solcoverjs ./.solcover.js",
    "typechain": "yarn hardhat typechain",
    "prettier:solidity": "./node_modules/.bin/prettier --write contracts/**/*.sol",
    "flatten": "mkdir -p contracts_flattened && npm run flatten:bonuz && npm run flatten:vesting",
    "flatten:bonuz": "npx hardhat flatten contracts/BONUZ.sol > contracts_flattened/BONUZ.sol",
    "flatten:vesting": "npx hardhat flatten contracts/vesting.sol > contracts_flattened/vesting.sol",
    "size": "npx hardhat size-contracts"
