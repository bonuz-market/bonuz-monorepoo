    "bonuzTokens:scripts:setIssuer:polygon": "npx hardhat run scripts/02.2.bonuz-tokens-setIssuer.ts --network polygon",
    "bonuzTokens:scripts:mintVoucher:polygon": "npx hardhat run scripts/02.3.bonuz-tokens-mint-voucher.ts --network polygon",
    "bonuzTokens:scripts:mintLoyalty:polygon": "npx hardhat run scripts/02.4.bonuz-tokens-mint-loyalty.ts --network polygon",
