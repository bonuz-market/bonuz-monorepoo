    "bonuzSocialId:deploy:arbitrum_nova": "npx hardhat deploy --network arbitrum_nova --tags BonuzSocialId",
    "bonuzSocialId:scripts:arbitrum_nova": "npx hardhat run scripts/01.bonuz-social-id.ts --network arbitrum_nova",
