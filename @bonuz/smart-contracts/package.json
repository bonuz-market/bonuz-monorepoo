    "bonuzTokens:scripts:mintPop:polygon": "npx hardhat run scripts/02.7.bonuz-tokens-mint-pop.ts --network polygon",
    "bonuzTokens:scripts:getTokenMetadata:polygon": "npx hardhat run scripts/02.8.bonuz-tokens-getTokenMetadata.ts --network polygon",
